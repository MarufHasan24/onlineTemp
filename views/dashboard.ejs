                                                                        if (!username) {
                                                                            removeChilds()
                                                                        } else
                                                                            if (jurrylist.includes(username) || username === userdata?.host) {
                                                                                //do nothing

                                                                            }
                                                                    } else if (f === "Jurries update") {
                                                                        // Show info to logged-in users, only juries can make changes
                                                                        if (username) {
                                                                            if (jurrylist.includes(username)) {
                                                                                //do nothing
                                                                            } else {
                                                                                removeChilds();
                                                                            }
                                                                        } else {
                                                                            document.body.removeChild(container)
